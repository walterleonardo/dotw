{
    "name": "dotw/protobuf-php-services",
    "type": "project",
    "license": "MIT",
    "autoload": {
    	"psr-4": {
    		"": "genProto",
            "protobuffer\\dotwproto\\": "srcProto"
    	}
    },
    "scripts": {
    	"gen-proto": "protoc --php_out=./genProto ./genProto/Dotw_ProtoBuf.proto",
        "gen-proto-cpp": "protoc --cpp_out=./genProto ./genProto/Dotw_ProtoBuf.proto"
    },
    "require": {
        "google/protobuf": "^3.5.1"
    }
}
